<?xml version="1.0" encoding="utf-8" ?>
<actions>
  <action id="NuPeek.OpenFromNuget" text="Open from &amp;NuGet..." image="JetBrains.DotPeek.Plugins.NuPeek.NuGet.png">
    <shortcut>Control+Shift+N</shortcut>
    <vs-shortcut>Control+Shift+N</vs-shortcut>
  </action>
  <action id="NuPeek.OpenFromNugetPackagesConfig" text="Open NuGet packages.config...">
  </action>

  <insert group-id="DotPeek.File" position="after" anchor-id="OpenFromGac">
    <action-ref id="NuPeek.OpenFromNuget" />
    <action-ref id="NuPeek.OpenFromNugetPackagesConfig" />
  </insert>

  <insert group-id="Toolbar" position="after" anchor-id="OpenFromGac">
    <action-ref id="NuPeek.OpenFromNuget" />
  </insert>
</actions>